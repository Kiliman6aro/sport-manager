<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="ua.pp.hophey.pushupapp.MainController">
    <children>
        <Label fx:id="welcomeText" layoutX="120.0" layoutY="30.0" text="Готовы начать тренировку?" />
        <TextField fx:id="setCountField" layoutX="120.0" layoutY="80.0" promptText="Количество подходов" />
        <Button fx:id="startButton" layoutX="120.0" layoutY="120.0" text="Старт тренировки" onAction="#onStartButtonClick" />
        <Label fx:id="timerLabel" layoutX="120.0" layoutY="160.0" text="00:00" />
    </children>
</AnchorPane>
